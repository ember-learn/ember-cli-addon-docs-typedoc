    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0",
    "typescript": ">= 4.6"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
